    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.DarkActionBar"/>
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowBackground">@color/bg_window</item>
    </style>
    <style name="button" parent="label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/height_button</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="editor" parent="label">
        <item name="android:textColorHint">@color/text_hint</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_edit</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>
    <style name="label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/height_label</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/font_normal</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">@color/text_normal</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textStyle">normal</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="list">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:clickable">false</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="splitter">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_splitter</item>
        <item name="android:background">@color/bg_splitter</item>
        <item name="android:layout_marginTop">@dimen/margin_splitter</item>
        <item name="android:layout_marginBottom">@dimen/margin_window</item>
    </style>
</resources>